{
    "version": 2,
    "name": "car-wash-typeshi",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/api-bridge",
            "dest": "/api/api-bridge-vercel.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}