{
    "version": 2,
    "name": "car-wash-typeshi",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "api/diagnostico-vercel.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/api-bridge",
            "dest": "/api/diagnostico-vercel.js"
        },
        {
            "src": "/api/diagnostico",
            "dest": "/api/diagnostico-vercel.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "functions": {
        "api/diagnostico-vercel.js": {
            "maxDuration": 10
        }
    }
}
