{
    "version": 2,
    "builds": [
        { "src": "api-bridge.js", "use": "@vercel/node" },
        { "src": "api/**/*.js", "use": "@vercel/node" },
        { "src": "src/frontend/**/*", "use": "@vercel/static" }
    ],    "routes": [
        { "src": "/api-bridge", "dest": "/api-bridge.js" },
        { "src": "/api-bridge(.*)", "dest": "/api-bridge.js$1" },
        { "src": "/api/bookings/available-slots(.*)", "dest": "/api/bookings/available-slots.js$1" },
        { "src": "/", "dest": "/src/frontend/index.html" },
        { "src": "/api/(.*)", "dest": "/api/$1" },
        { "src": "/(.*)", "dest": "/src/frontend/$1" }
    ],"env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api-bridge.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}