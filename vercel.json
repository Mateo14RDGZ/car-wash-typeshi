{
    "version": 2,
    "name": "car-wash-typeshi",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/api-bridge",
            "dest": "/api/api-bridge/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 30
        }
    }
}