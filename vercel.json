{
    "version": 2,
    "name": "car-wash-typeshi",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "api/api-bridge.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/api-bridge.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "functions": {
        "api/api-bridge.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}
